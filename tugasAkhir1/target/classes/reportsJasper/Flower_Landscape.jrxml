	<subDataset name="PieChartDataset" uuid="21b70311-b650-4678-a39c-887917366bf0"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    p.id_pesanan,
    m.nama_makanan, 
    q.quantity,
    q.harga_satuan,
    (q.quantity * q.harga_satuan) AS subtotal,
    p.hari_pengambilan,
    p.waktu_pengambilan,
    p.total_harga,
    p.status
FROM detail_pesanan AS q
JOIN pesanan AS p ON q.id_pesanan = p.id_pesanan
JOIN makanan AS m ON q.id_makanan = m.id_makanan --
WHERE p.status = 'Selesai'
ORDER BY p.id_pesanan ASC]]>
	</queryString>
